  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch main package",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      // "args": ["-p", "5001"],
      "showLog": true
    },
    {
      "name": "Launch test package",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${fileDirname}"
    },
    {
      "name": "Launch test file",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${file}"
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${input:processId}"
    }
  ],
  "inputs": [
    {
      "type": "promptString",
      "id": "processId",
      "description": "Process ID"
    }
  ]
}
